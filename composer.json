{
    "name": "zeasly/base-sdk",
    "description": "sdk基础框架, 就是 https://github.com/overtrue/wechat 的简化版",
    "keywords": [
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "xiaoze",
            "email": "zeasly@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "easywechat-composer/easywechat-composer": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.22",
        "pimple/pimple": "^3.0",
        "symfony/cache": "^3.3|^4.0",
        "symfony/http-foundation": "^2.7|^3.0|^4.0",
        "symfony/psr-http-message-bridge": "^0.3|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "^1.0",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "BaseSdk\\": "src/"
        },
        "files": [
            "src/Kernel/Support/Helpers.php",
            "src/Kernel/Helpers.php"
        ]
    }
}
